{
  "expo": {
    "name": "Kaffiene",
    "slug": "kaffiene",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Kaffiene-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Kaffiene-logo.png",
