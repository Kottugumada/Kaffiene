    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Kaffiene-logo.png",
        "backgroundColor": "#2A1412"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
